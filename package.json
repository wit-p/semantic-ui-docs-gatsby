{
  "name": "semantic-ui-docs-gatsby",
  "private": true,
  "description": "Documentation for the Semantic UI component library.",
  "version": "0.2.0",
  "author": "Ryosuke <ryosuke.san.hana@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.13.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-less": "^5.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-mdx": "^2.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-source-filesystem": "^3.13.0",
    "less": "^4.1.1",
    "node-sass": "^6.0.1",
    "prism-react-renderer": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-live": "^2.3.0",
    "react-syntax-highlighter": "^15.4.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "html-loader": "^2.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  },
  "keywords": [
    "react",
    "gatsby",
    "mdx",
    "semantic-ui",
    "semantic-ui-react",
    "documentation"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && npm run develop",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --ext ts --ext tsx",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*",
      "content/*",
      "static/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whoisryosuke/semantic-ui-docs-gatsby"
  },
  "bugs": {
    "url": "https://github.com/whoisryosuke/semantic-ui-docs-gatsby/issues"
  }
}
